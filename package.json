{
    "name": "meetcode-ui",
    "version": "0.0.11",
    "private": false,
    "files": [
        "lib/*"
    ],
    "main": "lib/meetcode-ui.umd.js",
    "module": "lib/meetcode-ui.esm.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build:site": "npm run clean:site && vite build",
        "build:packages": "npm run clean:packages && npm run build:esm && npm run build:umd",
        "build:esm": "rollup --config ./build/rollup.esm.config.js",
        "build:umd": "rollup --config ./build/rollup.umd.config.js",
        "clean:packages": "rimraf ./lib",
        "clean:site": "rimraf ./dist",
        "serve": "vite preview",
        "new": "sh scripts/new.sh"
    },
    "dependencies": {
        "@antfu/utils": "^0.3.0",
        "@vicons/ionicons5": "^0.11.0",
        "@vue/compiler-sfc": "^3.2.21",
        "@vueuse/components": "^6.7.4",
        "@vueuse/core": "^6.7.4",
        "@vueuse/integrations": "^6.7.5",
        "csstype": "^3.0.9",
        "monaco-editor": "^0.27.0",
        "monaco-editor-core": "^0.30.1",
        "naive-ui": "^2.21.1",
        "node-sass": "^6.0.1",
        "postcss": "^8.3.8",
        "sass": "^1.43.4",
        "splitpanes": "^3.0.4",
        "sucrase": "^3.20.3",
        "tailwindcss": "^2.2.16",
        "theme-vitesse": "^0.1.13",
        "vfonts": "^0.1.0",
        "vue": "^3.2.16",
        "vue-router": "^4.0.12",
        "vueuc": "^0.4.15",
        "windicss": "^3.2.1"
    },
    "devDependencies": {
        "@types/markdown-it": "^12.2.3",
        "@types/markdown-it-container": "^2.0.4",
        "@types/markdown-it-highlightjs": "^3.3.1",
        "@types/node": "^16.11.7",
        "@types/splitpanes": "^2.2.1",
        "@vicons/ionicons5": "^0.11.0",
        "@vitejs/plugin-vue": "^1.9.4",
        "markdown-it": "^12.2.0",
        "markdown-it-anchor": "^8.4.1",
        "markdown-it-container": "^3.0.0",
        "markdown-it-highlightjs": "^3.5.0",
        "postcss-import": "^14.0.2",
        "rollup": "^2.59.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-postcss": "^4.0.1",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-vue": "^6.0.0",
        "typescript": "^4.4.3",
        "vite": "^2.6.14",
        "vite-plugin-compression": "^0.3.5",
        "vite-plugin-md": "^0.11.0",
        "vue-tsc": "^0.3.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TonyL1u/meetcode-ui"
    },
    "keywords": [
        "meetcode-ui",
        "vue3",
        "script setup",
        "ui framework"
    ]
}
