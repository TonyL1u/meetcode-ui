        "node-sass": "^6.0.1",
        "postcss": "^8.3.8",
        "rollup": "^2.59.0",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-scss": "^3.0.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-vue": "^6.0.0",
        "sass": "^1.43.4",
        "tailwindcss": "^2.2.16",
        "vue": "^3.2.16",
        "vueuc": "^0.4.15"
    },
    "devDependencies": {
        "typescript": "^4.4.3",
        "vue-tsc": "^0.3.0"
